{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "strict": true,
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "prismaClient": ["prisma/generated"],
      "@exceptions": ["src/exceptions"],
      "@input": ["src/dto"],
      "@profile-error-messages": ["src/profile/profileErrorMessage"],
      "@profile-service": ["src/profile/profile.service"],
      "@profile-module": ["src/profile/profile.module"],
      "@content-discovery": ["src/content-discovery"],
      "@posts-newsfeed": ["src/newsfeed-posts"],
      "@posts-newsfeed/*": ["src/newsfeed-posts/*"],
      "@profile": ["src/profile"],
      "@comments": ["src/comments"],
      "@comments/*": ["src/comments/*"],
      "@reaction": ["src/reaction"],
      "@entities": ["src/entities"],
      "prismaService": ["src/prisma.service"],
      "@keys": ["src/keys"],
      "@content-management": ["src/content-management"],
      "@prisma-module": ["src/app.module"],
      "@block/*": ["src/block/*"],
      "@block": ["src/block"],
      "@privacy": ["src/privacy"],
      "@privacy-settings/*": ["src/privacy/*"],
      "@product-post/*": ["src/product-post/*"],
      "@affiliation-post/*": ["src/affiliation-post/*"],
      "@action/*": ["src/action/*"],
      "@hidden-content/*": ["src/hidden-content/*"],
      "@story/*": ["src/story/*"],
      "@kafkaModule": ["src/kafka.module"]
    }
  }
}
