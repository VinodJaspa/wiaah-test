  "scripts": {
    "build": "nx run-many --target=build",
    "dev": "nx run-many --target=dev --parallel=4 --exclude=nest-dto",
    "start:all": "concurrently \"pnpm --filter auth start:dev\" \"pnpm --filter accounts start:dev\" \"pnpm --filter products start:dev\" \"pnpm --filter reviews start:dev\" \"pnpm --filter search start:dev\" \"pnpm --filter services start:dev\" \"pnpm --filter chat start:dev\" \"pnpm --filter mailing start:dev\" \"pnpm --filter moderation start:dev\" \"pnpm --filter membership start:dev\" \"pnpm --filter billing start:dev\" \"pnpm --filter shopping-cart start:dev\" \"pnpm --filter affiliation start:dev\" \"pnpm --filter social start:dev\" \"pnpm --filter orders start:dev\" \"pnpm --filter hashtag start:dev\" \"pnpm --filter currency-conversion start:dev\"",
    "admin:build": "nx run admin:build",
    "admin:dev": "nx run admin:serve",
    "seller:build": "nx run seller:build",
    "seller:dev": "nx run seller:serve",
    "buyer:build": "nx run buyer:build",
    "buyer:dev": "nx run buyer:serve",
    "market:build": "nx run market:build",
    "market:dev": "nx run market:serve",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install || true",
    "sonar": "sonar-scanner -Dsonar.login=admin",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "extract-translations": "i18next",
    "codegen": "graphql-codegen --config codegen.ts",
    "cypress": "cypress open",
    "clean": "bash ./scripts/clean.sh"
  },
