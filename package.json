{
  "name": "wiaah-mono",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "sonar": "sonar-scanner -Dsonar.login=admin",
    "storybook": "set NODE_OPTIONS=--max_old_space_size=4096 && start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "extract-translations": "i18next"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@chakra-ui/storybook-addon": "^1.0.3",
    "@lingui/cli": "^3.13.2",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "12.1.2",
    "@types/enzyme": "^3.10.11",
    "@types/jest": "^27.4.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "husky": "^7.0.4",
    "i18next-parser": "^5.4.0",
    "jest": "^27.4.7",
    "jest-sonar-reporter": "^2.0.0",
    "lint-staged": "^12.2.2",
    "postcss": "^8.4.6",
    "react-test-renderer": "^17.0.2",
    "sonarqube-scanner": "^2.8.1",
    "storybook-react-i18next": "^1.0.17",
    "tailwindcss": "^3.0.20",
    "ts-jest": "^27.1.3",
    "turbo": "latest",
    "webpack": "^5.68.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "turbo": {
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "dist/**",
          ".next/**"
        ]
      },
      "lint": {
        "outputs": []
      },
      "dev": {
        "cache": false
      }
    }
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.7",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@tailwindcss/forms": "^0.4.0",
    "blob-util": "^2.0.2",
    "country-state-city": "^3.0.1",
    "formik": "^2.2.9",
    "framer-motion": "^6",
    "i18next": "^21.6.11",
    "i18next-browser-languagedetector": "^6.1.3",
    "next-pwa": "^5.4.5",
    "react-cookie": "^4.1.1",
    "react-flag-kit": "^1.0.0",
    "react-i18next": "^11.15.5",
    "react-query": "^3.35.0",
    "react-select": "^5.2.2",
    "react-webcam": "^6.0.1",
    "swiper": "^8.0.5",
    "yup": "^0.32.11"
  },
  "resolutions": {
    "webpack": "^5"
  }
}
